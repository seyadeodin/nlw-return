generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Feedback {
  id String @id @default(uuid())
  //id Int @id @default(autoincrement())

  type       String
  //type @db.VarChar(191) specfic databse format(postgres)
  comment    String
  screenshot String?

  @@map("feedbacks")
}

